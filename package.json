    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.3",
    "govuk-frontend": "^4.3.1",
    "jest": "^29.0.3",
    "jest-axe": "^6.0.0",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.34.2",
    "react-router-dom": "^6.3.0",
    "rollup-plugin-scss": "^3.0.0",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-router-dom": ">=6.3.0"
